  "name": "community-notes-writer",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "typecheck-elo": "cd src/elo-rating-web && tsc --noEmit",
    "test-airtable": "bun run src/test-airtable-logger.ts",
    "routine": "bun run src/scripts/createNotesRoutine.ts",
    "test": "bun run src/scripts/run.ts",
    "build-elo": "cd src/elo-rating-web && bun build ./app.ts ./airtableClient.ts ./eloCalculator.ts ./types.ts ./cacheManager.ts --outdir ./dist --target browser",
    "serve-elo": "bun run src/elo-rating-web/server.ts",
    "elo": "bun run build-elo && bun run serve-elo",
    "prompt-lab": "bun src/prompt-lab/server.ts",
    "prompt-lab:fetch": "bun src/prompt-lab/fetchAirtableData.ts",
    "bad-miss": "bun src/bad-miss-analysis/server.ts",
    "note-viewer": "bun src/tools/note-viewer/server.ts"
  }
