{
  "dependencies": {
    "@ai-sdk/openai": "latest",
    "@clack/prompts": "^0.11.0",
    "@openrouter/ai-sdk-provider": "latest",
    "@tonerow/agent-framework": "0.1.0",
    "@types/express": "^5.0.3",
    "@types/p-queue": "^3.2.1",
    "ai": "latest",
    "airtable": "^0.12.2",
    "axios": "^1.10.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "oauth-1.0a": "^2.2.6",
    "open": "^10.2.0",
    "openai": "^5.10.1",
    "p-queue": "^8.1.0",
    "playwright": "^1.54.1",
    "vercel": "^44.4.3",
    "zod": "^4.0.5"
  },
  "name": "community-notes-writer",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "scripts": {
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "typecheck-elo": "cd src/tools/elo-rating-web && tsc --noEmit",
    "test-airtable": "bun run src/test-airtable-logger.ts",
    "routine": "bun run src/scripts/createNotesRoutine.ts",
    "test": "bun run src/scripts/run.ts",
    "test-tweet": "bun run src/scripts/testSpecificTweet.ts",
    "fetch-notes": "bun run src/scripts/fetchOurNotes.ts",
    "backfill-notes": "bun run src/scripts/backfillNoteStatus.ts",
    "backfill-url-scores": "bun run src/scripts/backfillUrlScores.ts",
    "backfill-url-scores:test": "bun run src/scripts/backfillUrlScores.ts --test",
    "update-notes": "bun run src/scripts/updateNoteStatus.ts",
    "build-elo": "cd src/tools/elo-rating-web && bun build ./app.ts ./airtableClient.ts ./eloCalculator.ts ./types.ts ./cacheManager.ts --outdir ./dist --target browser",
    "watch-elo": "cd src/tools/elo-rating-web && bun build ./app.ts ./airtableClient.ts ./eloCalculator.ts ./types.ts ./cacheManager.ts --outdir ./dist --target browser --watch",
    "serve-elo": "bun run src/tools/elo-rating-web/server.ts",
    "elo": "bun run build-elo && bun run serve-elo",
    "elo:watch": "bun src/tools/elo-rating-web/watch.ts",
    "dev-elo": "bash src/tools/elo-rating-web/dev.sh",
    "prompt-lab": "bun src/tools/prompt-lab/server.ts",
    "prompt-lab:fetch": "bun src/tools/prompt-lab/fetchAirtableData.ts",
    "bad-miss": "bun src/tools/bad-miss-analysis/server.ts",
    "filter-lab": "bun src/tools/filter-lab/server.ts",
    "note-viewer": "bun --watch src/tools/note-viewer/server.ts",
    "test:log": "bun src/scripts/runTestsAndLog.ts",
    "test:positive-filter": "bun src/scripts/testPositiveFilter.ts"
  }
}
