    "@types/express": "^5.0.3",
    "@types/p-queue": "^3.2.1",
    "ai": "^4",
    "airtable": "^0.12.2",
    "axios": "^1.10.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "oauth-1.0a": "^2.2.6",
